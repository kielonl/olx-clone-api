generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id          String  @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  first_name  String
  second_name String
  password    String
  e_mail      String  @map("e-mail")
  profile_pic String?
}
